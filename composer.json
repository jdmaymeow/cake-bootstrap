{
    "name": "jdmaymeow/cake-bootstrap",
    "description": "Bootstrap plugin for CakePHP",
    "type": "cakephp-plugin",
    "config": {
        "secure-http": false
    },
    "require": {
        "php": ">=5.5.9",
        "cakephp/cakephp": ">=3.3.2 <4.0.0",
        "jdmaymeow/cake-font-awesome": "~1.0",
        "jdmaymeow/cake-highlight": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "CakeBootstrap\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CakeBootstrap\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
